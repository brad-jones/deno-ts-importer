{
  "name": "@brad-jones/deno-ts-importer",
  "version": "0.2.2",
  "license": "MIT",
  "publish": {
    "include": [
      "lib/**/*.ts",
      "README.md",
      "CHANGELOG.md"
    ],
    "exclude": [
      ".script/**",
      "*.test.ts",
      "*_bench.ts",
      "*_bench_*.ts"
    ]
  },
  "exclude": [
    ".coverage/**",
    "lib/testdata/**"
  ],
  "exports": {
    ".": "./lib/mod.ts"
  },
  "imports": {
    "@brad-jones/deno-config": "jsr:@brad-jones/deno-config@^1.0.0",
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.8",
    "@core/unknownutil": "jsr:@core/unknownutil@^4.3.0",
    "@cspotcode/outdent": "jsr:@cspotcode/outdent@^0.8.0",
    "@david/dax": "jsr:@david/dax@^0.45.0",
    "@deno/esbuild-plugin": "jsr:@deno/esbuild-plugin@^1.2.1",
    "@deno/rolldown-plugin": "jsr:@deno/rolldown-plugin@^0.0.10",
    "@fcrozatier/type-strip": "jsr:@fcrozatier/type-strip@^1.4.1",
    "@lulu/deno-rolldown-plugin": "jsr:@lulu/deno-rolldown-plugin@^0.1.1",
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/testing": "jsr:@std/testing@^1.0.14",
    "@deno/graph": "jsr:@deno/graph@^0.96.2",
    "@std/expect": "jsr:@std/expect@^1.0.14",
    "@std/crypto": "jsr:@std/crypto@^1.0.5",
    "@std/path": "jsr:@std/path@^1.0.1",
    "@std/fs": "jsr:@std/fs@^1.0.1",
    "@deno/cache-dir": "jsr:@deno/cache-dir@^0.13.2",
    "esbuild": "npm:esbuild@^0.27.3",
    "rolldown": "npm:rolldown@^1.0.0-rc.3",
    "typescript": "npm:typescript@^5.9.3"
  },
  "fmt": {
    "exclude": [
      "lib/testdata"
    ]
  },
  "lint": {
    "exclude": [
      "lib/testdata"
    ]
  },
  "test": {
    "exclude": [
      "lib/testdata"
    ]
  }
}